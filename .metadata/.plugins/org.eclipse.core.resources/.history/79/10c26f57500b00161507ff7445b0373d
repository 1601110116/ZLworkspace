#include <iostream>
#include <omp.h>
#include <math.h>
using namespace std;

int main() {

	int a=1;

	double t0=omp_get_wtime();

	for (int i = 0; i < 1e8; ++i) {
		a=a*2;
		a-=1
	}


	cout<<"a="<<a<<endl;

	cout<<"Total Time:"<<(omp_get_wtime()-t0)*1000<<endl;
	return 0;
}
