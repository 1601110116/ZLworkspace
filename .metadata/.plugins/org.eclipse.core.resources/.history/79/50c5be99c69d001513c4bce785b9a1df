#include "G4EmStandardPhysics.hh"

B1PhysicsList::B1PhysicsList():G4VModularPhysicsList() {
	SetVerboseLevel(1);
	RegisterPhysics(new G4EmStandardPhysics());
}

B1PhysicsList::~B1PhysicsList() {
}

void B1PhysicsList::SetCuts(){
	//SetCutsWithDefault();
	G4VUserPhysicsList::SetCuts();
}
